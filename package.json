{
  "name": "jest-md-dashboard",
  "version": "0.6.0",
  "description": "Generating a pretty Markdown dashboard for Jest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshrtsr/jest-md-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/mshrtsr/jest-md-dashboard/issues"
  },
  "homepage": "https://github.com/mshrtsr/jest-md-dashboard#readme",
  "author": "Masaharu TASHIRO <masatsr.kit@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jest",
    "reporter",
    "markdown"
  ],
  "type": "module",
  "exports": "./lib/index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prebuild": "run-s clean",
    "build": "tsc --build tsconfig.json --force",
    "start": "run-s 'build -w'",
    "test": "jest",
    "test:generate-dashboard": "jest --config=./jest.config.dashboard.js",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .js,.mjs,.cjs,.ts,.mts,.cts ./",
    "lint:md": "prettier --check ./**/*.md",
    "fix": "run-s fix:*",
    "fix:eslint": "run-s 'lint:eslint --fix'",
    "fix:md": "run-s 'lint:md --write'",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "isomorphic-git": "^1.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cybozu/eslint-config": "^17.0.2",
    "@jest/globals": "^28.1.2",
    "@jest/reporters": "^28.1.2",
    "@jest/test-result": "^28.1.1",
    "@jest/types": "^28.1.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/parser": "^5.30.4",
    "babel-jest": "^28.1.2",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.2.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.2",
    "jest-cli": "^28.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "release-please": "^13.19.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
